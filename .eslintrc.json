{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "linebreak-style": 0,
        "no-console": 0,
        "import/no-extraneous-dependencies": 0,
        "no-const-assign": 0,
        "no-shadow": 0
    }
}
